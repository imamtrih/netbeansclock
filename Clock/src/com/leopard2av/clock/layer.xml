<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Actions">
        <folder name="Tools">
            <file name="com-leopard2av-clock-ClockAction.instance">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="com.leopard2av.clock.Bundle"/>
                <attr name="delegate" newvalue="com.leopard2av.clock.ClockAction"/>
                <attr name="displayName" bundlevalue="com.leopard2av.clock.Bundle#CTL_ClockAction"/>
                <attr name="iconBase" stringvalue="com/leopard2av/clock/hora.png"/>
                <!--attr name="instanceCreate" methodvalue="org.openide.awt.Actions.alwaysEnabled"/-->
                <attr name="noIconInMenu" stringvalue="false"/>
            </file>
        </folder>
    </folder>
    <folder name="Toolbars">
        <folder name="QuickSearch">
            <file name="com-leopard2av-clock-ClockAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Tools/com-leopard2av-clock-ClockAction.instance"/>
                <attr name="position" intvalue="150"/>
            </file>
        </folder>
    </folder>
</filesystem>
